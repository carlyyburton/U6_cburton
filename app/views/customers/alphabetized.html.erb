<h2>Customers Alphabetized</h2>
<% @customers = Customer.order("full_name ASC").each do |customer| %>
  <p><%= customer.full_name %></p>
<% end %>
<div id="links">
  <%= link_to "All Customers", customers_path %>
  <br>
  <%= link_to "Customers Missing an Email", missing_email_path %>
</div>